{
  "extends": ["plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "always"
      }
    ]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
