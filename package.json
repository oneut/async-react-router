{
  "name": "async-react-router",
  "version": "0.2.9",
  "description": "Client side react router with async. It like next.js!",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:ssr",
    "build:lib": "rimraf lib && babel src/lib -d lib/",
    "build:ssr": "rimraf ssr && babel src/ssr -d ssr/",
    "lint": "eslint ./",
    "prepublish": "rimraf build && babel -d build/ src/",
    "test": "nyc ava --verbose",
    "report": "nyc report --reporter=html"
  },
  "files": [
    "lib",
    "ssr"
  ],
  "peerDependencies": {
    "react": "^15",
    "rxjs": "^5.0.0"
  },
  "dependencies": {
    "history": "^4.7.2",
    "path-to-regexp": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "browser-env": "^3.2.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "just-extend": "1.1.22",
    "nyc": "^11.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.3",
    "sinon": "^4.0.1",
    "webpack": "^3.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oneut/async-react-router.git"
  },
  "keywords": [
    "react",
    "router",
    "next.js",
    "async"
  ],
  "author": "onetu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oneut/async-react-router/issues"
  },
  "homepage": "https://github.com/oneut/async-react-router#readme",
  "ava": {
    "concurrency": 5,
    "powerAssert": true,
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  }
}
